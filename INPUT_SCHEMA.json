{
  "schemaVersion": 1,
  "title": "Facebook Ads Scraper - Input schema",
  "type": "object",
  "properties": {
    "keyword": {
      "type": "string",
      "title": "Keyword",
      "description": "Produto / termo de pesquisa",
      "editor": "textfield"
    },
    "country": {
      "type": "string",
      "title": "Country",
      "default": "ALL",
      "description": "ISO2 country code (ES, IT, RO) or ALL",
      "editor": "textfield"
    },
    "maxResults": {
      "type": "integer",
      "title": "Max results",
      "default": 50
    }
  },
  "required": ["keyword"]
}
